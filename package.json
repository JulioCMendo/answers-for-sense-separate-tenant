{
  "name": "answers-for-sense",
  "version": "0.1.0",
  "description": "This extension allows you to embed a Qlik Answers assistant in your Qlik Sense app.",
  "license": "MIT",
  "author": "Riley MacDonald",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.10.0"
  },
  "main": "dist/answers-for-sense.js",
  "module": "dist/answers-for-sense.esm.js",
  "scripts": {
    "build": "nebula build",
    "lint": "eslint src",
    "start": "nebula serve",
    "sense": "nebula build && nebula sense --meta src/meta.json && cd .. && python3 build.py extension",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report test/integration/test-report"
  },
  "devDependencies": {
    "@nebula.js/cli": "5.2.0",
    "@nebula.js/cli-build": "5.2.0",
    "@nebula.js/cli-sense": "5.2.0",
    "@nebula.js/cli-serve": "5.2.0",
    "@playwright/test": "^1.28.1",
    "@qlik/sdk": "^0.12.0",
    "eslint": "7.27.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-mocha": "9.0.0"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=5.0.0"
  }
}
